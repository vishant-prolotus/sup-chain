        <script>
            $(document).ready(function () {

                $(".notifications-menu").click(function() {
                    $.get( "/removeNotifications", function( data ) {
                        $("#notiCount").remove();
                    });
                });


                $("#state").val("<%=comDoc.registeredAddress.State%>");
                $("#c-state").val("<%=comDoc.correspondenceAddress.State%>");

                $("#cin").blur(function() {
                    $.get("https://data.gov.in/api/datastore/resource.json?resource_id=7502bd54-2f04-43a5-ae40-437628b0145a&api-key=8f16543fe5d2b4558223e29c2da31b08&filters[COMPANYNAME]="+$("#companyName").val()+"&fields=COMPANYNAME,REGISTEREDOFFICEADDRESS,CORPORATEIDENTIFICATIONNUMBER,DATEOFREGISTRATION,COMPANYCATEGORY,AUTHORIZEDCAPITAL,PAIDUPCAPITAL,COMPANYSTATUS,PRINCIPALBUSINESSACTIVITY,REGISTRAROFCOMPANIES,SUBCATEGORY", function (response) {
                        if(response.count>0) {
                            $("#companyName").val(response.records[0]["COMPANYNAME"]);
                            $("#street").val(response.records[0]["REGISTEREDOFFICEADDRESS"]);



                            varPin = (response.records[0]["REGISTEREDOFFICEADDRESS"]).split(" ");
                            varP = varPin[(varPin.length - 1)];
                            $("#pin").val(varP);



                            txtCompany = "The Company, <strong>"+response.records[0]["COMPANYNAME"]+"</strong>, Incorporated on <strong>"+response.records[0]["DATEOFREGISTRATION"]+"</strong>,";
                            txtCompany = txtCompany + " having its registered office at <strong>'"+response.records[0]["REGISTEREDOFFICEADDRESS"];
                            txtCompany = txtCompany + "'</strong>, Registered with <strong>"+response.records[0]["REGISTRAROFCOMPANIES"]+"</strong> is a <strong>"+response.records[0]["COMPANYCATEGORY"]+"</strong> <strong>"+response.records[0]["SUBCATEGORY"]+"</strong>,";
                            txtCompany = txtCompany + " with an authorised capital and paid up capital of <strong>"+response.records[0]["AUTHORIZEDCAPITAL"]+"</strong> and <strong>"+response.records[0]["PAIDUPCAPITAL"]+"</strong> respectively.";
                            txtCompany = txtCompany + "The current status of the Company is <strong>"+response.records[0]["COMPANYSTATUS"]+"</strong> and its Principal business activity is <strong>"+response.records[0]["PRINCIPALBUSINESSACTIVITY"]+"</strong>";
                            $("#hCompany").val(txtCompany);




                        }
                    }, "json");
                });


                $("#chkAddress").click(function() {
                    if($(this).is(":checked")) {
                        $("#c-street").val($("#street").val());
                        $("#c-city").val($("#city").val());
                        $("#c-state").val($("#state").val());
                        $("#c-pin").val($("#pin").val());
                    }
                    else {
                        $("#c-street").val("");
                        $("#c-city").val("");
                        $("#c-state").val("");
                        $("#c-pin").val("");
                    }
                });

            });
        </script>


