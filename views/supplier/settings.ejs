        =====================================================================-->
        <!-- Start Theme label Script
        =====================================================================-->


        <!-- ChartJs JavaScript -->
        <script src="/assets/plugins/chartJs/Chart.min.js" type="text/javascript"></script>



        <!-- Dashboard js -->
        <script src="/assets/dist/js/dashboard.js" type="text/javascript"></script>
        <!-- End Theme label Script
        =====================================================================-->
        <script>
            $(document).ready(function () {

                "use strict"; // Start of use strict




                var ctx = document.getElementById("singelBarChart");
                var myChart = new Chart(ctx, {
                    type: 'bar',
                    data: {
                        labels: ["Block Height"],
                        datasets: [
                            {
                                label: "Block Height (<%=blockData.height%>)",
                                data: [<%=blockData.height%>],
                                borderColor: "rgba(55, 160, 0, 0.9)",
                                borderWidth: "0",
                                backgroundColor: "rgba(55, 160, 0, 0.5)"
                            }
                        ]
                    },
                    options: {
                        scales: {
                            yAxes: [{
                                    ticks: {
                                        beginAtZero: true
                                    }
                                }]
                        }
                    }
                });

                $(".btnViewTansaction").click(function() {
                    var url = "/check?Tid="+$(this).attr("id");
                    $.get( url, function( data ) {
                    $( ".result" ).html( data );
                        $("#chainId").text(data.chaincodeID);
                        $("#payload").text(data.payload);
                        $("#txid").text(data.txid);
                        $("#timestamp").text(data.timestamp.seconds);
                        $("#nonce").text(data.nonce);
                        $("#cert").text(data.cert);
                        $("#signature").text(data.signature);
                        //$("#chainId").val(data.chaincodeID);    
                        //alert( data.chaincodeID );
                         $('#myModal').modal('show');
                    });
                   
                });



                $(".notifications-menu").click(function() {
                    $.get( "/removeNotifications", function( data ) {
                        $("#notiCount").remove();
                    });
                });

            });
        </script>


</body>
