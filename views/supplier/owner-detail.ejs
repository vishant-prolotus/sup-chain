                </figure>
            </div>
        </div>
    </div>
 </div>
 


    <% include ../partials/footer %>





    <% include ../partials/bottom %>



    

    <style>
        th {
            padding: 14px !important;
        }
        .form-pannel fieldset {
            border: 1px solid #ccc;
            min-height: 325px;
            padding: 10px;
            margin-bottom: 10px;
        }
        .mandatory-txt {
            color: #ff0000;
            float: left;
        }
        .mandatory {
            color: #ff0000;
        }
    </style>

    <script>
        $(document).ready(function () {

            $("#btnAddMore").click(function() {
                $('#addMore').append($("#divOwner").html());


                

                for(i = 0; i < $(".txtPan").length; i = i + 1) {
                    $(".txtPan").eq(i).attr("name", $(".txtPan").eq(i).attr("name").replace('Pan-0', 'Pan-'+i));
                }

                for(i = 0; i < $(".txtAddress").length; i = i + 1) {
                    $(".txtAddress").eq(i).attr("name", $(".txtAddress").eq(i).attr("name").replace('Address-0', 'Address-'+i));
                }

                for(i = 0; i < $(".txtPhoto").length; i = i + 1) {
                    $(".txtPhoto").eq(i).attr("name", $(".txtPhoto").eq(i).attr("name").replace('Photo-0', 'Photo-'+i));
                }
            });


            $("#verfiyAadhaar").click(function() {

                if($("#txtAadhar").val()!="") {
                    winAadhaar = window.open("http://localhost:3202/supplier/get-aadhaar", "", "width=900,height=500,left=600,top=300");
                }
                else {
                    alert("Please Enter Aadhaar No.");
                }
            });


            $("#btnSubmit").click(function() {
                if($("#chkAuthorize").is(":checked")) {
                    if($("#chkAuthorize").is(":checked")) {
                        $("#frmOwner").submit();
                    }
                    else {
                        alert("Please Accept Terms of Use & Privacy Policy");
                    }
                }
                else {
                    alert("Please Accept to pull consumer CIBIL.");
                }
            });
        });
    </script>




</body>
</html>