                                                                    <th>Buyer Name</th>
                                                                    <th>Supplier Name</th>
                                                                    <th >Disbursed Amont</th>
                                                                    <th>
                                                                        Action
                                                                    </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>

                                                                <% if (accData.dInvoices!=null){%>
