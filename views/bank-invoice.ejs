                                                        <table  class="table table-striped dataTableExample2">
                                                            <thead>
                                                                <tr>
                                                                    <th>#Invoice</th>
                                                                    <th>Offer Date</th>
                                                                    <th>Buyer Name</th>
                                                                    <th>Supplier Name</th>
                                                                    <th>Discount Rate (APR)</th>
                                                                    <th>#Days</th>
