                                <label for="txtCreditPeriod">Invoice Amount</label>
                                <input type="text" name="invAmount" id="invAmount" value="" class="form-control" readonly>
                                <input type="hidden" name="invID" id="invID" value="">
                                <input type="hidden" name="buyer" id="buyer" value="">
                                <input type="hidden" name="supplier" id="supplier" value="">
                            </div>
                            <div class="form-group">
                                <label for="txtCreditPeriod">Discount Rate (%)</label>
                                <input type="text" name="discountRate" id="discountRate" value="" class="form-control" readonly>
                            </div>
                            <div class="form-group">
                                <label for="txtCreditPeriod">No of Days</label>
                                <input type="text" name="noDays" id="noDays" value="" class="form-control" readonly>
                            </div>
                            <div class="form-group">
                                <label for="penaltyPerDay">Penalty per Day</label>
                                <input type="text" name="penaltyPerDay" id="penaltyPerDay" value="" class="form-control" >
                            </div>
                            <div class="form-group">
                                                <label for="exampleSelect1">Recourse</label>
