                <!-- Main content -->
                <section class="content">
                    <div class="row">



                        <div class="col-xs-12 col-sm-12 col-md-12 m-b-20">
                            <!-- Nav tabs -->
                            <ul class="nav nav-tabs">
                                <li class="active"><a href="#tab1" data-toggle="tab">Invoices</a></li>
                                <li><a href="#tab3" data-toggle="tab">Offers</a></li>
                                <li><a href="#tab2" data-toggle="tab">Disbursed Invoices</a></li>
                            </ul>
                            <!-- Tab panels -->
                            <div class="tab-content">
                                <div class="tab-pane fade in active" id="tab1">
                                    <div class="panel-body">
                                        <div>
                                            <div class="panel panel-bd">
                                                <div class="panel-heading">
                                                    <div class="panel-title">
                                                        <h4>Invoices</h4>
                                                    </div>
                                                </div>
                                                <div class="panel-body">
                                                    <div class="table-responsive">
                                                        <table id="dataTableExample2" class="table table-striped dataTableExample2">
                                                            <thead>
                                                                <tr>
                                                                    <th>#Invoice</th>
                                                                    <th>Invoice Date</th>
                                                                    <th>Buyer Name</th>
                                                                    <th>Supplier Name</th>
                                                                    <th style="text-align: right;">Total</th>
                                                                    <th style="width: 245px;">
                                                                        Action
                                                                    </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>    
                                                                <%if (invoices!=null){%>
                                                                <% for (var i=0;i<invoices.length;i++) { 
                                                                    var status=0;
                                                                    if (accData.offers){
                                                                    for (var k=0;k<accData.offers.length;k++){
                                                                        if (accData.offers[k].details.details.invoiceId==invoices[i].invoiceId){
                                                                            status=status+1;
                                                                        }
                                                                    }
                                                                    }
                                                                    %>
                                                                    <% if (invoices[i].status=="approved"&&status==0) { %>
                                                                    <tr>
                                                                    <td>
                                                                        <a href="/bank/view-invoice?poid=<%=invoices[i].invoiceId.slice(0,(invoices[i].invoiceId.length))%>&supplier=<%=suppliers[i]%>&buyer=<%=buyers[i]%>"><%=invoices[i].invoiceId%></a>
                                                                    </td>
                                                                    <td>
                                                                        <%=invoices[i].date%>
                                                                        <%=invoices[i].time%>
                                                                    </td>
                                                                    <td><%=buyers[i]%></td>
                                                                    <td><%=suppliers[i]%></td>
                                                                    <td style="text-align: right;"><%=configData.currency%> <%=invoices[i].total.toFixed(2)%></td>
                                                                    <td>
                                                                        <a href="javascript:;" id="<%=invoices[i].invoiceId.slice(0,(invoices[i].invoiceId.length))%>###<%=invoices[i].total%>###<%=invoices[i].buyer%>###<%=invoices[i].supplier%>" class="btn btn-warning w-md m-b-5 btn-sm btnOffer">Generate Offer</a>
                                                                        <%var supplierIcount=0;%>
                                                                        <% for (var z=0;z<userData.supplierInvoiceLimits.length;z++){%>
                                                                            <%if (userData.supplierInvoiceLimits[z].supplierId==invoices[i].supplier) {supplierIcount++; %>
                                                                                <%if (userData.supplierInvoiceLimits[z].limit<invoices[i].total){ %>
                                                                                    Red Flag1 
                                                                                <%}else {%>
                                                                                <%var supplierTcount=0%>
                                                                                    <% for (var m=0;m<userData.supplierLimits.length;m++){%>
                                                                                        <%if (userData.supplierLimits[m].supplierId==invoices[i].supplier){supplierTcount++;%>
                                                                                            <%if (userData.supplierLimits[m].limit>=invoices[i].total){%>
                                                                                                Green Flag1
                                                                                            <%}else {%>
                                                                                                Red Flag2
                                                                                            <%}%>
                                                                                        <%}%>
                                                                                    
                                                                                    <%}%>
                                                                                    <%if (userData.supplierLimits.length==0||supplierTcount==0){%>
                                                                                        Green Flag2
                                                                                    <%}%>

                                                                                <%}%>
                                                                            <%}%>
                                                                        <%}%>
                                                                        <%if (userData.supplierInvoiceLimits.length==0||supplierIcount==0){%>
                                                                            Green Flag3
                                                                        <%}%>
                                                                        <!--<a href="javascript:;" id="<%=invoices[i].invoiceId.slice(1,(invoices[i].invoiceId.length))%>###<%=invoices[i].total%>###<%=invoices[i].buyer%>###<%=invoices[i].supplier%>" class="btn btn-primary btn-sm btnDisburseAmount">Disburse Invoice</a>-->
                                                                    </td>
                                                                    </tr>
                                                                    <% } %>
                                                                <% } }%>
                                                                
                                                            </tbody>
                                                        </table>
