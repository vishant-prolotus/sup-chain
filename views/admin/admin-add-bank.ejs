<body class="hold-transition sidebar-mini">


        <!-- Site wrapper -->
        <div class="wrapper">
            <% include ../partials/admin_header %>
            <!-- =============================================== -->
            <!-- Left side column. contains the sidebar -->
            <% include ../partials/admin_left %>
            <!-- =============================================== -->
            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                <!-- Content Header (Page header) -->
                <section class="content-header">
                    <div class="header-icon">
                        <i class="pe-7s-world"></i>
                    </div>
                    <div class="header-title">
                        <h1>SofoSupply - Add Lender</h1>
                        <small>You can add lender</small>
                        <ol class="breadcrumb">
                            <li><a href="/admin"><i class="pe-7s-home"></i> Home</a></li>
                            <li class="active">Add Lender</li>
                        </ol>
                    </div>
                </section>
                <!-- Main content -->
                <section class="content">
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                            <div class="panel panel-bd">
                                <div class="panel-heading">
                                    <div class="panel-title">
                                        <h4>Add Lender</h4>
                                    </div>
                                </div>
                                <div class="panel-body">
                                    <form action="/admin/manage-bank" method="post">
                                        <div class="form-group">
                                            <label for="txtLenderName">Lender Name</label>
                                            <input class="form-control" name="name" id="name" placeholder="Enter Lender's Name" type="text">
                                        </div>
                                        <div class="form-group">
                                            <label for="txtLenderEmail">Email</label>
                                            <input class="form-control" name="email" id="email" aria-describedby="emailHelp" placeholder="Enter Email" type="email">
                                            <small id="emailHelp" class="text-muted">We'll never share your email with anyone else.</small>
                                        </div>
                                        <div class="form-group">
                                            <label for="txtLenderEmail">Employee Name</label>
                                            <input class="form-control" name="employee" id="employee"  placeholder="Enter Full Name" type="text">
                                        </div>
                                        <div class="form-group">
                                            <label for="txtPassword">Password</label>
                                            <input class="form-control" name="password" id="password" placeholder="Password" type="password">
                                        </div>
                                        <div class="form-group">
                                            <label for="txtPassword">Contact Phone Number</label>
                                            <input class="form-control" name="phone" id="phone" placeholder="Enter Phone number" type="text">
                                        </div>


                                        
                                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
                                            <div class="panel-heading">
                                                <div class="panel-title">
                                                    <h4 style="font-size:30px;">Loan Terms</h4>
                                                </div>
                                            </div>
                                            <div class="panel-body">
                                                <div class="form-group">
                                                    <label for="txtPassword">Minimum Age of Business</label>
                                                    <input class="form-control" name="minAge" id="minAge" placeholder="enter minimum age of business applicable for loan" type="number">
                                                </div>
                                                <div class="form-group">
                                                    <label for="txtPassword">Minimum Annual Revenue</label>
                                                    <input class="form-control" name="minRevenue" id="minRevenue" placeholder="enter minimum annual revenue of business applicable for loan" type="number">
                                                </div>
                                                <div class="form-group">
                                                    <label for="txtPassword">DSER</label>
                                                    <input class="form-control" name="dser" id="dser" placeholder="DSER" type="text">
                                                </div>
                                                <div class="form-group">
                                                    <label for="txtPassword">Geographical Footprint</label>
                                                    <input class="form-control" name="geoFootprint" id="geoFootprint" placeholder="Enter Geographical Footprint" type="text">
                                                </div>
                                            </div>
                                        </div>





                                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
                                            <div class="panel-heading">
                                                <div class="panel-title">
                                                    <h4 style="font-size:30px;">Policy Parameters</h4>
                                                </div>
                                            </div>
                                            <div class="panel-body">
                                                <div class="form-group">
                                                    <label for="txtPassword">Invoice Funding %</label>
                                                    <input class="form-control" name="fundRate" id="fundRate" placeholder="Default funding rate" type="text">
                                                </div>
                                                <div class="form-group">
                                                    <label for="txtPassword">Interest Charges</label>
                                                        <select class="form-control" id="interestCharges">                    
                                                            <option value="Upfront">Upfront</option>
                                                            <option value="Later(On Repayment)">Later(On Repayment)</option>
                                                        </select>        
                                                </div>
                                                
                                                <div class="form-group">
                                                        <label for="exampleSelect1">Recourse</label>
                                                        <select class="form-control" id="recourse">
                                                            <option value="">None</option>
                                                            <option value="Seller">Seller</option>
                                                            <option value="Buyer">Buyer</option>
                                                        </select>
                                                </div>
                                            </div>
                                        </div>
                                            
                                        
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                                <button type="submit" class="btn btn-primary" style="clear: both;">Submit</button>
                                            <div>
                                        </div>
                                    </form>
                                </div>                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
                            </div>
                        </div>





                        




                    </div> <!-- /.row -->
                </section> <!-- /.content -->
            </div> <!-- /.content-wrapper -->
            <% include ../partials/admin_footer %>
        </div>
        <!-- ./wrapper -->
